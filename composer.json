{
	"name": "scriptotek/moccamapper",
	"description": "UI for making mppings between a source vocabulary and several target vocabularies",
	"keywords": [],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
        "artdarek/oauth-4-laravel": "dev-master",
        "jenssegers/mongodb": "dev-master",
        "willdurand/negotiation": "1.2.*",
        "felixkiss/uniquewith-validator": "1.0.*",
        "wikibase/easyrdf_lite": "dev-master"
	},
    "require-dev": {
        "way/generators": "2.*",
        "loic-sharma/profiler": "dev-master"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
            "php artisan config:publish artdarek/oauth-4-laravel"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
